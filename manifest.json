{
  "name": "FocaLearn",
  "description": "An app to help you stay focused on your studies.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.js"
  },
  "options_page": "options.html",
  "permissions": ["storage", "activeTab", "scripting", "notifications"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "/logos/focalearn-logo.png"
    }
  }
}
